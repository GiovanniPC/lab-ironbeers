<div class="card" >
  <img class="card-img-top" src={{this.image_url}} alt="Card image cap">
  <div class="card-body">
    <h3 class="card-title">{{this.name}} </h3>
    <p class="card-text" style="color:rgba(145, 145, 145, 0.481);">{{this.tagline}}</p>
    <p class="card-text">{{this.description}}</p>
  </div>
</div>